              <Javascript>// ----- SERVO PORTS -----
var wrist  = 16;
var thumb  = 21;
var index  = 20;
var middle = 19;
var ring   = 18;
var pinky  = 17;

// ----- left hand SERVO PORTS -----
var l_wrist  = 10;
var l_thumb  = 4;
var l_index  = 3;
var l_middle = 2;
var l_ring   = 1;
var l_pinky  = 0;

var minAngle = 5;
var midAngle = 79;
var maxAngle = 175;

// ----- HAND SHAPES -----
function rockHand() {
  Servo.setPosition(thumb, 180);
  Servo.setPosition(index, 180);
  Servo.setPosition(middle, 180);
  Servo.setPosition(ring, 180);
  Servo.setPosition(pinky, 180);
}

function l_rockHand() {
  Servo.setPosition(l_thumb, 180);
  Servo.setPosition(l_index, 180);
  Servo.setPosition(l_middle, 180);
  Servo.setPosition(l_ring, 180);
  Servo.setPosition(l_pinky, 180);
}


function paperHand() {
  Servo.setPosition(thumb, 0);
  Servo.setPosition(index, 0);
  Servo.setPosition(middle, 0);
  Servo.setPosition(ring, 0);
  Servo.setPosition(pinky, 0);
}

function l_paperHand() {
  Servo.setPosition(l_thumb, 0);
  Servo.setPosition(l_index, 0);
  Servo.setPosition(l_middle, 0);
  Servo.setPosition(l_ring, 0);
  Servo.setPosition(l_pinky, 0);
}

function l_scissorsHand() {
  Servo.setPosition(l_thumb, 180);
  Servo.setPosition(l_index, 0);
  Servo.setPosition(l_middle, 0);
  Servo.setPosition(l_ring, 180);
  Servo.setPosition(l_pinky, 180);
}

function scissorsHand() {
  Servo.setPosition(thumb, 180);
  Servo.setPosition(index, 0);
  Servo.setPosition(middle, 0);
  Servo.setPosition(ring, 180);
  Servo.setPosition(pinky, 180);
}

function shakeHand() {
  for (var i = 0; i &lt; 3; i++) {
    Servo.setPosition(wrist, minAngle);
    sleep(1000);
    Servo.setPosition(wrist, maxAngle);
    sleep(1000);
  }
  Servo.setPosition(wrist, midAngle);
}

function l_shakeHand() {
  for (var i = 0; i &lt; 3; i++) {
    Servo.setPosition(l_wrist, minAngle);
    sleep(1000);
    Servo.setPosition(l_wrist, maxAngle);
    sleep(1000);
  }
  Servo.setPosition(l_wrist, midAngle);
}

