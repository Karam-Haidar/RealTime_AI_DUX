          <Description />
          <EZScript />
          <BlocklyXML />
          <Javascript>// ==================================================
// ========== TORSO &amp; ARMS SERVO PORTS (EZ-B v4) ======
// ==================================================


// -------------------------------
// ‚úÖ RIGHT ARM SERVOS
// -------------------------------

// ---- RIGHT ELEVATION ----
var r_elevation = 13;
var R_ELEV_MIN = 6;
var R_ELEV_MId = 21;
var R_ELEV_MAX = 70;

var minAngle = 5;
var midAngle = 79;
var maxAngle = 175;

// ---- RIGHT SHOULDER UP / DOWN ----
var r_shoulder = 14;
var R_SHOULDER_MIN = 6;
var R_SHOULDER_MIN = 50;
var R_SHOULDER_MAX = 106;

// ---- RIGHT BICEP ROTATION ----
var r_bicep = 15;
var R_BICEP_MIN = 1;
var R_BICEP_MID = 90;
var R_BICEP_MAX = 180;

// ---- RIGHT ELBOW ----
var r_elbow = 16;
var R_ELBOW_MIN = 9;     // straight
var R_ELBOW_MID = 90;    // right angle
var R_ELBOW_MAX = 125;   // folded


// -------------------------------
// ‚úÖ LEFT ARM SERVOS////////////////////////////////////////////////////////////////////////
// -------------------------------

// ---- LEFT ELEVATION ----
var l_elevation = 7;
var L_ELEV_MIN = 121;   // hand up
var L_ELEV_MID = 151; 
var L_ELEV_MAX = 180;   // hand down

// ---- LEFT SHOULDER UP / DOWN ----
var l_shoulder = 8;
var L_SHOULDER_MIN = 80;
var L_SHOULDER_MIN = 50;
var L_SHOULDER_MAX = 180;

// ---- LEFT BICEP ROTATION ----
var l_bicep = 9;
var L_BICEP_MIN = 1;
var L_BICEP_MID = 88;
var L_BICEP_MAX = 141;

// ---- LEFT ELBOW ----
var l_elbow = 10;
var L_ELBOW_MIN = 90;
var L_ELBOW_MAX = 180;


var headLR = "1.D0";
var HEAD_LEFT  = 63;
var HEAD_MID   = 87;
var HEAD_RIGHT = 111;


var headUD = "1.D6";
var HEAD_UP  = 63;
var HEAD_Intial   = 45;
var HEAD_DOWN = 65;


// ---- EYES LEFT / RIGHT ----
var eyesLR = "1.D1";
var EYES_LEFT  = 128;
var EYES_MID   = 90;
var EYES_RIGHT = 55;

// ---- EYES UP / DOWN ----
var eyesUD = "1.D0";
var EYES_UP   = 55;
var EYES_MID2 = 100;
var EYES_DOWN = 150;

// ---- JAW (MOUTH) ----
var jaw = "1.D2";
var JAW_CLOSED = 1;
var JAW_OPEN   = 60;

// ---- NECK TILT LEFT ----
var neckTiltLeft = "1.D5";
var NTL_MIN = 1;     // fully extended
var NTL_MID = 35;
var NTL_MAX = 78;    // fully retracted

// ---- NECK TILT RIGHT ----
var neckTiltRight = "1.D4";
var NTR_MIN = 34;    // fully extended
var NTR_MID = 68;
var NTR_MAX = 157;   // fully retracted

// ---- NECK FORWARD / BACK ----
var neckFB = "1.D6";
var NECK_UP   = 1;
var NECK_MIDDLE = 30;
var NECK_DOWN = 65;





function lookRight() {
  Servo.setPosition(0, 111, 0);
}

function lookLeft() {
  Servo.setPosition(0, 71,0);
}

function lookCenter() {
  Servo.setPosition(0, 87,0);
}



function lookUp() {
  Servo.setPosition(headUD, HEAD_UP);
}

function lookDown() {
  Servo.setPosition(6, 65,0);
}

function lookForward() {
  Servo.setPosition(6, 45, 0);   /// Servo.setPosition(headUD, HEAD_Intial);
}



function eyesRight() {
  Servo.setPosition(eyesLR, EYES_RIGHT);
}

function eyesLeft() {
  Servo.setPosition(eyesLR, EYES_LEFT);
}

function eyesCenter() {
  Servo.setPosition(eyesLR, EYES_MID);
}

function eyesUp() {
  Servo.setPosition(eyesUD, EYES_UP);
}

function eyesDown() {
  Servo.setPosition(eyesUD, EYES_DOWN);
}

function mouthOpen() {
  Servo.setPosition(jaw, JAW_OPEN);
}

function mouthClose() {
  Servo.setPosition(jaw, JAW_CLOSED);
}

function shakeYes() {
  Servo.setPosition(neckFB, NECK_DOWN);
  sleep(300);
  Servo.setPosition(neckFB, NECK_UP);
}

function shakeNo() {
  lookLeft();
  sleep(200);
  lookRight();
  sleep(200);
  lookCenter();
}

function elbow_shake1() {
  for (var i = 0; i &lt; 1; i++) {
    Servo.setPosition(16, 72,1);
    sleep(1000);
    Servo.setPosition(16, 65,1);
    sleep(1000);
  }
  Servo.setPosition(16, 65,1);
}

function r_elbow_shake1_RPS2() {
  for (var i = 0; i &lt; 1; i++) {
    Servo.setPosition(16, 76,1);
    sleep(1000);
    Servo.setPosition(16, 65,1);
    sleep(1000);
  }
  Servo.setPosition(16, 76,1);
}

// ---- RIGHT ARM ----
function r_elbowUp()     { Servo.setPosition(16, 76,1); }
function r_elbowMid()    { Servo.setPosition(r_elbow, R_ELBOW_MID); }
function r_elbowDown()   { Servo.setPosition(r_elbow, R_ELBOW_MIN); }



function r_shoulderUp()  { Servo.setPosition(r_shoulder, R_SHOULDER_MAX); }
function r_shoulderMid() { Servo.setPosition(r_shoulder, R_SHOULDER_MID); }
function r_shoulderDown(){ Servo.setPosition(r_shoulder, R_SHOULDER_MIN); }

function r_elevUp()      { Servo.setPosition(13, 34, 1); }
