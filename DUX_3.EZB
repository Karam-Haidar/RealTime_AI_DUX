  "You are a Rock Paper Scissors judge. " +
  "Look at the most recent camera image from the robot and decide " +
  "which hand shape is shown. " +
  "The ONLY valid answers are: Rock, Paper, or Scissors. " +
  "Rules: Do not explain. Do not describe the image. " +
  "Do not add any extra words or punctuation. " +
  "Return exactly ONE word: Rock OR Paper OR Scissors.";

var startCheck = "";

while (true) {

  // Ask Autonomous AI
  controlCommand("Autonomous AI", "Send", rps_prompt);

  // Wait for response
  sleep(1000);

  // Read AI result
  startCheck = getVar("$AutonomousAIResponse", "");

  if (startCheck == "Rock") {
    controlCommand("Azure Text To Speech", "speak", "Perfect. I see a rock, so you're ready let's Start!");
    setVar("$head_motion", "HeadCenter");
    //setVar("$RPS_motion", "HeadIntail");

