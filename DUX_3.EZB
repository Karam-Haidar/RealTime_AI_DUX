        if (idx === -1) idx = maxLen;
        parts.push(text.substring(0, idx + 1).trim());
        text = text.substring(idx + 1).trim();
    }
    if (text.length &gt; 0) {
        parts.push(text.trim());
    }
    return parts;
}

