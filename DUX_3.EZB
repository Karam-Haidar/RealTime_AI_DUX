// ----- HAND SHAPES -----
function rockHand() {
  Servo.setPosition(thumb, 180, 1);
  Servo.setPosition(index, 180,1);
  Servo.setPosition(middle, 180,1);
  Servo.setPosition(ring, 180,1);
  Servo.setPosition(pinky, 180,1);
}

function l_rockHand() {
  Servo.setPosition(l_thumb, 180);
  Servo.setPosition(l_index, 180);
  Servo.setPosition(l_middle, 180);
  Servo.setPosition(l_ring, 180);
  Servo.setPosition(l_pinky, 180);
}


function paperHand() {
  Servo.setPosition(thumb, 0,1,1);
  Servo.setPosition(index, 0,1,1);
  Servo.setPosition(middle, 0,1,1);
  Servo.setPosition(ring, 0,1,1);
  Servo.setPosition(pinky, 0,1,1);
}

function l_paperHand() {
  Servo.setPosition(l_thumb, 0,1);
  Servo.setPosition(l_index, 0,1);
  Servo.setPosition(l_middle, 0,1);
  Servo.setPosition(l_ring, 0,1);
  Servo.setPosition(l_pinky, 0,1);
}

function l_scissorsHand() {
  Servo.setPosition(l_thumb, 180);
  Servo.setPosition(l_index, 0);
  Servo.setPosition(l_middle, 0);
  Servo.setPosition(l_ring, 180);
  Servo.setPosition(l_pinky, 180);
}

function scissorsHand() {
  Servo.setPosition(thumb, 180,1);
  Servo.setPosition(index, 0,1);
  Servo.setPosition(middle, 0,1);
  Servo.setPosition(ring, 180,1);
  Servo.setPosition(pinky, 180,1);
}

function shakeHand() {
  for (var i = 0; i &lt; 3; i++) {
    Servo.setPosition(wrist, minAngle,1);
    sleep(1000);
    Servo.setPosition(wrist, maxAngle,1);
    sleep(1000);
  }
  Servo.setPosition(wrist, midAngle,1);
}

function l_shakeHand() {
  for (var i = 0; i &lt; 3; i++) {
    Servo.setPosition(l_wrist, minAngle,1);
    sleep(1000);
    Servo.setPosition(l_wrist, maxAngle,1);
    sleep(1000);
  }
  Servo.setPosition(l_wrist, midAngle,1);
}

function decideWinner(robotMove, challengerMove) {
  if (robotMove == challengerMove) return "It is a tie.";
  if (robotMove == "rock" &amp;&amp; challengerMove == "scissors") return "I win!";
  if (robotMove == "scissors" &amp;&amp; challengerMove == "paper") return "I win!";
  if (robotMove == "paper" &amp;&amp; challengerMove == "rock") return "I win!";
  return "You win!";
}
